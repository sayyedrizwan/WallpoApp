<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/black"
    tools:context=".fragment.ProfileFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardview"
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:layout_margin="15dp"
            app:cardCornerRadius="9dp">

            <ImageView
                android:id="@+id/profilepic"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@mipmap/profilepic" />

        </androidx.cardview.widget.CardView>

        <RelativeLayout
            android:id="@+id/dislay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="22dp"
            android:layout_toEndOf="@+id/cardview"
            android:layout_toRightOf="@+id/cardview">

            <TextView
                android:id="@+id/displayname"
                style="@style/TextAppearance.AppCompat.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_margin="2dp"
                android:fontFamily="@font/lovelo"
                android:gravity="center"
                android:text=""
                android:textColor="@color/white"
                android:textStyle="bold" />

        </RelativeLayout>

        <LinearLayout
            android:id="@+id/postsplace"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/dislay"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="10dp"
            android:layout_toEndOf="@+id/cardview"
            android:layout_toRightOf="@+id/cardview"
            android:orientation="horizontal">

            <androidx.cardview.widget.CardView
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:foreground="?android:attr/selectableItemBackground"
                app:cardElevation="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tvPosts"
                        android:textColor="@android:color/black"
                        style="@style/TextAppearance.AppCompat.Medium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:layout_marginRight="16dp"
                        android:text="@string/posts" />


                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:foreground="?android:attr/selectableItemBackground"
                app:cardElevation="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/cardsignup"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textPosts"
                        style="@style/TextAppearance.AppCompat.Medium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textColor="@color/white" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

        </LinearLayout>

        <HorizontalScrollView
            android:id="@+id/scrollview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/postsplace"
            android:layout_marginTop="40dp"
            android:scrollbars="none">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <androidx.cardview.widget.CardView
                    android:id="@+id/viewprofile"
                    android:layout_width="110dp"
                    android:layout_height="130dp"
                    android:layout_margin="10dp"
                    android:clickable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    app:cardCornerRadius="13dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/yellowlite"
                        android:orientation="vertical"
                        android:weightSum="10">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_margin="12dp"
                            android:layout_weight="8"
                            android:padding="12dp"
                            android:src="@drawable/ic_user" />

                        <TextView
                            style="@style/TextAppearance.AppCompat.Small"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_gravity="center"
                            android:layout_weight="2"
                            android:fontFamily="@font/worlsans"
                            android:text="@string/viewprofile"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:translationY="-2dp" />

                    </LinearLayout>


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/editprofile"
                    android:layout_width="110dp"
                    android:layout_height="130dp"
                    android:layout_margin="10dp"
                    android:clickable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    app:cardCornerRadius="13dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/yellowlite"
                        android:orientation="vertical"
                        android:weightSum="10">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_margin="12dp"
                            android:layout_weight="8"
                            android:padding="12dp"
                            android:src="@drawable/ic_edit" />

                        <TextView
                            style="@style/TextAppearance.AppCompat.Small"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_gravity="center"
                            android:layout_weight="2"
                            android:fontFamily="@font/worlsans"
                            android:text="@string/editprofile"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:translationY="-2dp" />

                    </LinearLayout>


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/upload"
                    android:layout_width="110dp"
                    android:layout_height="130dp"
                    android:layout_margin="10dp"
                    android:clickable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    app:cardCornerRadius="13dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:id="@+id/uploadlay"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/yellowlite"
                        android:orientation="vertical"
                        android:weightSum="10">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_margin="12dp"
                            android:layout_weight="8"
                            android:padding="12dp"
                            android:src="@drawable/ic_upload" />


                        <TextView
                            style="@style/TextAppearance.AppCompat.Small"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_gravity="center"
                            android:layout_weight="2"
                            android:fontFamily="@font/worlsans"
                            android:text="@string/upload"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:translationY="-2dp" />


                    </LinearLayout>

                    <RelativeLayout
                        android:id="@+id/uploadoptions"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/yellowlite"
                        android:orientation="vertical"
                        android:weightSum="10">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/image"
                            android:padding="3dp"
                            android:layout_centerHorizontal="true"
                            android:layout_width="53dp"
                            android:layout_alignParentTop="true"
                            android:layout_height="53dp"
                            android:layout_margin="8dp"
                            app:cardCornerRadius="90dp">

                            <ImageView
                                android:padding="3dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="8dp"
                                android:src="@drawable/ic_wallpaper_upload" />

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/video"
                            android:layout_centerHorizontal="true"
                            android:layout_width="53dp"
                            android:layout_height="53dp"
                            android:padding="3dp"
                            android:layout_alignParentBottom="true"
                            android:layout_margin="8dp"
                            app:cardCornerRadius="90dp">

                            <ImageView
                                android:padding="3dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="8dp"
                                android:src="@drawable/ic_wallpaper_video" />

                        </androidx.cardview.widget.CardView>


                    </RelativeLayout>


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/favourite"
                    android:layout_width="110dp"
                    android:layout_height="130dp"
                    android:layout_margin="10dp"
                    android:clickable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    app:cardCornerRadius="13dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/yellowlite"
                        android:orientation="vertical"
                        android:weightSum="10">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_margin="12dp"
                            android:layout_weight="8"
                            android:padding="12dp"
                            android:src="@drawable/ic_star" />

                        <TextView
                            style="@style/TextAppearance.AppCompat.Small"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_gravity="center"
                            android:layout_weight="2"
                            android:fontFamily="@font/worlsans"
                            android:text="@string/favourites"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:translationY="-2dp" />

                    </LinearLayout>


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/message"
                    android:layout_width="110dp"
                    android:layout_height="130dp"
                    android:layout_margin="10dp"
                    android:clickable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    app:cardCornerRadius="13dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/yellowlite"
                        android:orientation="vertical"
                        android:weightSum="10">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_margin="12dp"
                            android:layout_weight="8"
                            android:padding="12dp"
                            android:src="@drawable/ic_chat" />

                        <TextView
                            style="@style/TextAppearance.AppCompat.Small"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_gravity="center"
                            android:layout_weight="2"
                            android:fontFamily="@font/worlsans"
                            android:text="@string/messages"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:translationY="-2dp" />

                    </LinearLayout>


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/logout"
                    android:layout_width="110dp"
                    android:layout_height="130dp"
                    android:layout_margin="10dp"
                    android:clickable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    app:cardCornerRadius="13dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/yellowlite"
                        android:orientation="vertical"
                        android:weightSum="10">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_margin="12dp"
                            android:layout_weight="8"
                            android:padding="12dp"
                            android:src="@drawable/ic_logout" />

                        <TextView
                            style="@style/TextAppearance.AppCompat.Small"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_gravity="center"
                            android:layout_weight="2"
                            android:fontFamily="@font/worlsans"
                            android:text="@string/logout"
                            android:textColor="@android:color/black"
                            android:textStyle="bold"
                            android:translationY="-2dp" />

                    </LinearLayout>


                </androidx.cardview.widget.CardView>

            </LinearLayout>

        </HorizontalScrollView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/scrollview"
            android:layout_marginStart="50dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="50dp"
            android:layout_marginRight="50dp"
            android:background="@drawable/roundborderviewposts"
            android:orientation="vertical">

            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="19dp">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/premium"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="25dp"
                        android:layout_marginEnd="28dp"
                        android:layout_marginRight="28dp"
                        android:foreground="?android:attr/selectableItemBackground"
                        app:cardCornerRadius="10dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="8dp">

                            <TextView
                                android:id="@+id/premiumtext"
                                style="@style/TextAppearance.AppCompat.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:fontFamily="sans-serif"
                                android:text="@string/premiumads"
                                android:textColor="@android:color/black"
                                android:textStyle="bold" />


                            <ImageView
                                android:id="@+id/premiumarrow"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:layout_centerInParent="true"
                                android:layout_marginEnd="15dp"
                                android:layout_marginRight="15dp"
                                android:rotation="180"
                                android:src="@drawable/ic_left_arrow" />


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/multipleaccounts"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="25dp"
                        android:layout_marginEnd="28dp"
                        android:layout_marginRight="28dp"
                        android:foreground="?android:attr/selectableItemBackground"
                        app:cardCornerRadius="10dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="8dp">

                            <TextView
                                android:id="@+id/multipletext"
                                style="@style/TextAppearance.AppCompat.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:fontFamily="sans-serif"
                                android:text="@string/multipleaccounts"
                                android:textColor="@android:color/black"
                                android:textStyle="bold" />


                            <ImageView
                                android:id="@+id/multiplearrow"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:layout_centerInParent="true"
                                android:layout_marginEnd="15dp"
                                android:layout_marginRight="15dp"
                                android:rotation="180"
                                android:src="@drawable/ic_left_arrow" />


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>


                    <androidx.cardview.widget.CardView
                        android:id="@+id/wallpapersetting"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="25dp"
                        android:layout_marginEnd="28dp"
                        android:layout_marginRight="28dp"
                        android:foreground="?android:attr/selectableItemBackground"
                        app:cardCornerRadius="10dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="8dp">

                            <TextView
                                android:id="@+id/wallpapersetttext"
                                style="@style/TextAppearance.AppCompat.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:fontFamily="sans-serif"
                                android:text="@string/wallpapersettings"
                                android:textColor="@android:color/black"
                                android:textStyle="bold" />


                            <ImageView
                                android:id="@+id/wallpapersetarrow"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:layout_centerInParent="true"
                                android:layout_marginEnd="15dp"
                                android:layout_marginRight="15dp"
                                android:rotation="180"
                                android:src="@drawable/ic_left_arrow" />


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/suggestionsignin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="25dp"
                        android:layout_marginEnd="28dp"
                        android:layout_marginRight="28dp"
                        android:foreground="?android:attr/selectableItemBackground"
                        app:cardCornerRadius="10dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="8dp">

                            <TextView
                                android:id="@+id/suggestiontext"
                                style="@style/TextAppearance.AppCompat.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:fontFamily="sans-serif"
                                android:layout_centerVertical="true"
                                android:text="@string/suggestionnotification"
                                android:textColor="@android:color/black"
                                android:textStyle="bold" />


                            <com.google.android.material.switchmaterial.SwitchMaterial
                                android:id="@+id/notificationswitch"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:checked="true" />

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/changelanugau"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="25dp"
                        android:layout_marginEnd="28dp"
                        android:layout_marginRight="28dp"
                        android:foreground="?android:attr/selectableItemBackground"
                        app:cardCornerRadius="10dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="8dp">

                            <TextView
                                android:id="@+id/changetext"
                                style="@style/TextAppearance.AppCompat.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:fontFamily="sans-serif"
                                android:text="@string/changelanguage"
                                android:textColor="@android:color/black"
                                android:textStyle="bold" />


                            <ImageView
                                android:id="@+id/changearrow"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:layout_centerInParent="true"
                                android:layout_marginEnd="15dp"
                                android:layout_marginRight="15dp"
                                android:rotation="180"
                                android:src="@drawable/ic_left_arrow" />


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/likedsignin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="25dp"
                        android:layout_marginEnd="28dp"
                        android:layout_marginRight="28dp"
                        android:foreground="?android:attr/selectableItemBackground"
                        app:cardCornerRadius="10dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="8dp">

                            <TextView
                                android:id="@+id/likedtext"
                                style="@style/TextAppearance.AppCompat.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:fontFamily="sans-serif"
                                android:text="@string/likedposts"
                                android:textColor="@android:color/black"
                                android:textStyle="bold" />


                            <ImageView
                                android:id="@+id/likedarrow"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:layout_centerInParent="true"
                                android:layout_marginEnd="15dp"
                                android:layout_marginRight="15dp"
                                android:rotation="180"
                                android:src="@drawable/ic_left_arrow" />


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/twoauthsignin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="25dp"
                        android:layout_marginEnd="28dp"
                        android:layout_marginRight="28dp"
                        android:foreground="?android:attr/selectableItemBackground"
                        app:cardCornerRadius="10dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="8dp">

                            <TextView
                                android:id="@+id/twoauthtext"
                                style="@style/TextAppearance.AppCompat.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:fontFamily="sans-serif"
                                android:text="@string/twofactorauthentication"
                                android:textColor="@android:color/black"
                                android:textStyle="bold" />


                            <ImageView
                                android:id="@+id/twoautharrow"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:layout_centerInParent="true"
                                android:layout_marginEnd="15dp"
                                android:layout_marginRight="15dp"
                                android:rotation="180"
                                android:src="@drawable/ic_left_arrow" />


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/usedposts"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginLeft="28dp"
                        android:layout_marginTop="25dp"
                        android:layout_marginEnd="28dp"
                        android:layout_marginRight="28dp"
                        android:foreground="?android:attr/selectableItemBackground"
